    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "immer": "^7.0.8",
    "lodash": "^4.17.20",
    "normalizr": "^3.6.1",
    "rc-field-form": "^1.10.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0",
    "react-sidebar": "^3.0.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.0",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
