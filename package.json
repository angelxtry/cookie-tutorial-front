    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.22",
    "@types/react-sidebar": "^3.0.0",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "4.41.22",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
