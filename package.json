{
  "name": "cookie-tutorial-front",
  "version": "0.1.0",
  "description": "Build UI that scales",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "NODE_ENV=development webpack-dev-server --progress --hot",
    "aws:upload": "aws s3 sync ./dist s3://cookie-tutorial --profile=user",
    "aws:invalidate": "aws cloudfront create-invalidation --profile=user --distribution-id E1P2DDD6QVWDV --paths / /index.html /error.html /app.js /favicon.ico",
    "deploy": "yarn build && yarn aws:upload && yarn aws:invalidate"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
